<table class="table table-hover col-md-12">
    <thead class="analytics_table titles">
        <tr>
            <!-- TODO: Insert list of metrics here -->
            <th>Story ID</th>
            <th>Story State</th>
            <th>Discussion Time</th>
            <th>Vote Variance</th>
        </tr>
    </thead>
    <tbody class="stories story_rows">
        <% project.stories.each do |story| %>
            <tr class="analytics_table story session-<%= session_class(story) %>" style="display:<%= get_initial_display_table(story) %>">
                <td><%= story.id %></td>
                <td><%= story.current_state %></td>
                <td><%= Activity.discuss_time(story.id) %></td>
                <td><%= Activity.vote_variance(story.id) %></td>
            </tr>
        <% end %>
    </tbody>
</table>