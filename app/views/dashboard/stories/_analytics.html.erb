<table class="table table-hover col-md-12">
    <thead class="analytics_table titles">
        <tr>
            <!-- TODO: Insert list of metrics here -->
            <th>Story ID</th>
            <th>Metric 1</th>
            <th>Metric 2</th>
        </tr>
    </thead>
    <tbody class="analytics_table story_rows">
        <% project.stories.each do |story| %>
            <tr class="analytics_table story story-<%= estimation_class(story) %>" style="display:<%= get_initial_display_table(story) %>">
                <td><%= story.id %></td>
                <td><%= story.current_state %></td>
                <td><%= story.name %></td>
            </tr>
        <% end %>
    </tbody>
</table>